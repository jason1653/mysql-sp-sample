
	DECLARE _PI FLOAT;
	DECLARE _L FLOAT;
	DECLARE _K FLOAT;
	DECLARE _A1 FLOAT;
	DECLARE _A2 FLOAT;
	DECLARE _A3 FLOAT;
	DECLARE _A4 FLOAT;
	DECLARE _A5 FLOAT;
	DECLARE _CND FLOAT;
	
	
	SET _PI = 3.14159265358979;
	SET _A1 = 0.31938153;
    SET _A2 = -0.356563782;
    SET _A3 = 1.781477937;
    SET _A4 = -1.821255978;
    SET _A5 = 1.330274429;

	SET _L = ABS(_X);
	SET _K = 1 / (1 + 0.2316419 * _L);

	

	SET _CND = 1 / SQRT(2 * PI()) * EXP(-POWER(_X, 2) / 2);

	RETURN _CND;